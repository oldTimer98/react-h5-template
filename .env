# 应用标题（用于 HTML title）
VITE_APP_TITLE=React H5 Template

# 开发服务器端口
VITE_PORT=3000
# 是否自动打开浏览器
VITE_OPEN=false

# API 请求基础路径
VITE_API_BASE_URL=/api
# 代理目标地址（仅开发环境生效）
VITE_PROXY_TARGET=https://jsonplaceholder.typicode.com
# 请求超时时间（毫秒）
VITE_API_TIMEOUT=15000

# 移动端调试工具 eruda（true 启用）
VITE_ENABLE_ERUDA=false
# TanStack DevTools（Router + Query 调试面板）
VITE_ENABLE_DEVTOOLS=false

# 是否开启 gzip + brotli 压缩
VITE_BUILD_GZIP=false
# 是否删除 console/debugger
VITE_DROP_CONSOLE=false
# 打包输出目录
VITE_OUT_DIR=dist
